      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#242D75"
      },
      "package": "com.retray.econommy"
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
